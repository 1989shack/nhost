{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "build/**", ".next"]
    },
    "test": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "docgen": {
      "cache": false
    },
    "lint": { "outputs": [] },
    "lint:fix": { "outputs": [] }
  }
}
