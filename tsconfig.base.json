{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compileOnSave": false,
  "compilerOptions": {
    "declaration": true,
    "importHelpers": true,
    "strict": true,
    "lib": [
      "es5",
      "dom",
      "es2015.promise",
      "es2015.symbol",
      "es2015.iterable",
      "es2015.collection",
      "es2015.symbol.wellknown",
      "es2015.core",
      "es2017.object",
      "es2017.string"
    ],
    "module": "ES2015",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "target": "ES6",
    "typeRoots": [
      "./node_modules/@types"
    ]
  }
}